    "autoprefixer": "^10.4.16",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "naive-ui": "^2.35.0",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.10.0",
    "sass": "^1.69.5",
    "sm-crypto": "^0.3.13",
    "standard-version": "^9.5.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
