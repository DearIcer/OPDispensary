    "highlight.js": "^11.9.0",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "print-js": "^1.6.0",
    "qs": "^6.11.2",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.1",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@iconify/json": "^2.2.152",
    "@types/md5": "^2.3.5",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.10",
    "@types/sm-crypto": "^0.3.4",
    "@types/sortablejs": "^1.15.7",
